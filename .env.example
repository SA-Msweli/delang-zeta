# DeLangZeta Environment Variables

# Cloud Functions Configuration (Single Source of Truth)
VITE_CLOUD_FUNCTIONS_URL=https://us-central1-delang-zeta.cloudfunctions.net

# Google Cloud Project
VITE_GOOGLE_CLOUD_PROJECT=delang-zeta

# Blockchain Configuration
VITE_ZETACHAIN_RPC_URL=https://zetachain-evm.blockpi.network/v1/rpc/public
VITE_ZETACHAIN_TESTNET_RPC_URL=https://zetachain-athens-evm.blockpi.network/v1/rpc/public

# WalletConnect Configuration
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Development Configuration
VITE_ENVIRONMENT=development
VITE_DEBUG=true

# Cloud Functions Environment Variables (for deployment)
NODE_ENV=development
FRONTEND_URL=http://localhost:5173
GOOGLE_CLOUD_PROJECT=delang-zeta
JWT_SECRET=your-jwt-secret-key
CORS_ORIGIN=http://localhost:5173